/* 本页面的样式 */

page{
  background-color: rgb(49, 49, 54);
 
}
.container {
  background-color: rgb(49, 49, 54);
}

.box {
  width: 100%;
  height: 100%;
}

.img_a {
  width: 100%;
}

.content {
  border-bottom: 1px solid rgb(247, 198, 17);
  /* height: 40px; */
}

.section_icon {
  width: 85%;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
  margin-bottom: 5px;
  height: 30px;
  margin-top: 10px;
}

.section {

  width: 150px;
  height: 25px;
  text-align: center;
}

.input_text {
  width: 100%;
  height: 100%;
}

.button {
  width: 60px;
  border-radius: 5px;
  text-align: center;
  height: 25px;
  background-color: rgb(203, 127, 7);
  color: #fff;
}

.button_text {
  color: #fff;
}

.section_i {
  width: 85%;
  margin: 0 auto;
  margin-bottom: 5px;
  height: 30px;
  margin-top: 10px;
}
.input{
  width: 70%;
  margin: 0 auto;
}
.input_text{
  width: 100%;
  height: 100%;
}
.images{
display: flex;
width: 60%;
margin: 0 auto;
justify-content: center;
margin-top: 10px;
margin-bottom: 10px;
}
images_a{
width: 100%;
height: 100%;
}
.footer{
  display: flex;
width: 65%;
margin: 0 auto;
justify-content: center;
margin-top: 10px;
margin-bottom: 10px;
  background-color: rgb(203, 127, 7);
  height: 40px;
  text-align: center;
  border-radius: 5px;
}
.footer_text{
  color: #fff;
  text-align: center;
  height: 40px;
  line-height: 40px;
}
