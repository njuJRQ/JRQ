/* 本页面的样式 */

.container {
  background-color: white;
}

.box {
  width: 95%;
  margin: 0 auto;
  background-color: whitesmoke;
  padding-top: 10px;
  padding-bottom: 15px;
  /* border-radius: 10px; */
  margin-top: 15px;
  box-shadow: 0px 15px 30px rgba(0,0,0,0.1);
}

.vessel {
  width: 93%;
  display: flex;
  justify-content: space-between;
  margin: 0 auto;
}

.img {
  width: 120px;
  height: 80px;
  border-radius: 5px;
}

.img_bofang {
  height: 100%;
  width: 100%;
  border-radius: 5px;
}
.right {
  display: flex;
  flex: 1;
  flex-direction: column;
  justify-content: flex-start;
  margin-left: 10px;
}

.title {
  font-size: 12px;
  color: black;
}
.titles{
 font-size: 12px;
  color: black;
  margin-top: 5px; 
}
.shoping{
  /* border: 1px solid red; */
  display: flex;
  justify-content: space-between;
  text-align: center;
  padding-top: 30px;
}

.money {
  font-size: 22px;
  color: black;
  font-weight:800;
  text-align: center;
  margin-top: 10px;
}

.img_cart {
  width: 15px;
  height: 15px;
  margin-top: 10px;

}
